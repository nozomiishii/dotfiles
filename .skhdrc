# How to restart
# brew services restart skhd

# ctrl | shift | alt | cmd

# focus window
ctrl + shift - f : yabai -m window --focus stack.prev
ctrl + shift - d : yabai -m window --focus stack.next

# send window to monitor
ctrl + shift - g  : yabai -m window --display 1 && yabai -m space --stack 1
ctrl + shift - s  : yabai -m window --display 2 && yabai -m space --stack 2
