# How to restart
# brew services restart skhd

# ctrl | shift | alt | cmd

# focus window
shift + alt - f : yabai -m window --focus stack.prev
shift + alt - d : yabai -m window --focus stack.next

# send window to monitor
shift + alt - g  : yabai -m window --display 1 && yabai -m space --stack 1
shift + alt - s  : yabai -m window --display 2 && yabai -m space --stack 2
