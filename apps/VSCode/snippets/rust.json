{
  // ----------------------------------------------------------------
  // Pretty Console log
  // ----------------------------------------------------------------
  "pretty console log": {
    "prefix": "log",
    "body": ["dbg!(&`$CLIPBOARD);"],
    "description": "pretty console log"
  },
  "pretty console check": {
    "prefix": "log",
    "body": ["println!(\"[$TM_FILENAME:$TM_LINE_NUMBER] $CLIPBOARD: {:#?}\", $CLIPBOARD);"],
    "description": "pretty console check"
  }
}
